{
  "name": "jonassiewertsen/statamic-jobs",
  "description": "",
  "keywords": [
    "Statamic",
    "Addon",
    "Laravel"
  ],
  "license": "MIT",
  "type": "statamic-addon",
  "autoload": {
    "psr-4": {
      "Jonassiewertsen\\Jobs\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jonassiewertsen\\Jobs\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Jonas Siewertsen"
    }
  ],
  "support": {
    "email": "mail@jonassiewertsen.com"
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "statamic/cms": "^4.0|^5.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.1"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true,
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  },
  "extra": {
    "statamic": {
      "name": "Jobs",
      "description": ""
    },
    "laravel": {
      "providers": [
        "Jonassiewertsen\\Jobs\\ServiceProvider"
      ]
    }
  }
}
